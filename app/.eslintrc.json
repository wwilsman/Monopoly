{
  "env": {
    "browser": true
  },
  "extends": [
    "../.eslintrc.json",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "globals": {
    "require": true,
    "module": true,
    "process": true
  },
  "rules": {
    "react/jsx-indent": ["error", 2],
    "react/jsx-indent-props": ["error", 2]
  }
}
