{
  "name": "monopoly-wallet",
  "version": "0.0.2",
  "description": "Monopoly Wallet",
  "main": "lib/monopoly-game.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/wwilsman/monopoly-wallet.git"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "setup:db": "test -d data || mkdir data",
    "start": "npm run setup:db && npm run start:all",
    "start:all": "parallelshell \"npm run start:db\" \"npm run start:server\"",
    "start:db": "mongod --dbpath data --quiet",
    "start:server": "babel-node server.js"
  },
  "engines": {
    "node": "6.2.x"
  },
  "author": "Wil Wilsman <hello@wilwilsman.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "mocha": "^2.5.3",
    "parallelshell": "^2.0.0",
    "sinon": "^1.17.6",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bourbon": "^4.2.7",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "hbs": "^4.0.0",
    "jquery": "^3.1.0",
    "mongodb": "^2.1.18",
    "mongoskin": "^2.1.0",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-native-web": "0.0.44",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-form": "^6.0.0--alpha",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "redux-undo": "^0.6.1",
    "socket.io": "^1.4.8"
  }
}
